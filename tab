@Test
void should_call_processJsonDiff() {
    try (MockedStatic<JsonUtils> utilities = Mockito.mockStatic(JsonUtils.class)) {
        // Arrange : on définit le comportement
        utilities.when(() -> JsonUtils.processJsonDiff(anyString(), anyString()))
                 .thenReturn(List.of(new JsonDiffDto()));

        // Act : on exécute ton code
        schedulerEmailNotif.executeTaskEmailNotif();

        // Assert : on vérifie que la méthode statique a bien été appelée
        utilities.verify(() -> JsonUtils.processJsonDiff(anyString(), anyString()));
    }
}
